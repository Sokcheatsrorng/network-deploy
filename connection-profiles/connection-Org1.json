      "caName": "ca.Org1.idonate.istad.co",
      "tlsCACerts": {
        "path": "./org1Ca/tlsca/ca.org1.idonate.istad.co-cert.pem"
      },
      "httpOptions": {
        "verify": false
      },
      "registrar": [{
        "enrollId": "admin",
        "enrollSecret": "adminpw"
      }]
    }
  }
