{
  "name": "Org1_Network",
  "version": "1.0.0",
  "client": {
    "organization": "Org1",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    },
    "credentialStore": {
      "path": "./wallet",
      "cryptoStore": {
        "path": "./wallet"
      }
    }
  },
  "channels": {
    "idonatechannel": {
      "orderers": [
        "orderer1.Org1.idonate.istad.co",
        "orderer2.Org1.idonate.istad.co",
        "orderer3.Org1.idonate.istad.co"
      ],
      "peers": {
        "peer1.Org1.idonate.istad.co": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer2.Org1.idonate.istad.co": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        }
      }
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "peer1.Org1.idonate.istad.co",
        "peer2.Org1.idonate.istad.co"
      ],
      "certificateAuthorities": [
        "ca.Org1.idonate.istad.co"
      ]
    }
  },
  "orderers": {
    "orderer1.{Org1.idonate.istad.co}": {
      "url": "grpcs://orderer1.Org1.idonate.istad.co:7050",
      "tlsCACerts": {
        "path": "./crypto-config/ordererOrganizations/orderers/orderer1.Org1.idonate.istad.co/tls/ca.crt"
      },
      "grpcOptions": {
        "ssl-target-name-override": "orderer1.Org1.idonate.istad.co",
        "hostnameOverride": "orderer1.Org1.idonate.istad.co"
      }
    },
    "orderer2.Org1.idonate.istad.co": {
      "url": "grpcs://orderer2.Org1.idonate.istad.co:8050",
      "tlsCACerts": {
        "path": "./crypto-config/ordererOrganizations/orderers/orderer2.Org1.idonate.istad.co/tls/ca.crt"
      },
      "grpcOptions": {
        "ssl-target-name-override": "orderer2.Org1.idonate.istad.co",
        "hostnameOverride": "orderer2.Org1.idonate.istad.co"
      }
    },
    "orderer3.Org1.idonate.istad.co": {
      "url": "grpcs://orderer3.Org1.idonate.istad.co:9050",
      "tlsCACerts": {
        "path": "./crypto-config/ordererOrganizations/orderers/orderer3.Org1.idonate.istad.co/tls/ca.crt"
