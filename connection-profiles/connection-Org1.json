      "grpcOptions": {
        "ssl-target-name-override": "orderer3.Org1.idonate.istad.co",
        "hostnameOverride": "orderer3.Org1.idonate.istad.co"
      }
    }
  },
  "peers": {
    "peer1.Org1.idonate.istad.co": {
      "url": "grpcs://peer1.Org1.idonate.istad.co:7051",
      "tlsCACerts": {
        "path": "./crypto-config/peerOrganizations/peers/peer1.Org1.idonate.istad.co/tls/ca.crt"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer1.Org1.idonate.istad.co",
        "hostnameOverride": "peer1.Org1.idonate.istad.co"
      }
    },
    "peer2.Org1.idonate.istad.co": {
      "url": "grpcs://peer2.Org1.idonate.istad.co:7051",
      "tlsCACerts": {
        "path": "./crypto-config/peerOrganizations/peers/peer2.Org1.idonate.istad.co/tls/ca.crt"
